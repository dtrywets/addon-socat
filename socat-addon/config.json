{
  "name": "Socat Proxy",
  "version": "0.0.8",
  "slug": "socat-addon",
  "description": "Exposes socat to create virtual serial devices",
  "arch": ["aarch64", "amd64", "armv7", "i386"],
  "startup": "services",
  "boot": "auto",
  "hassio_api": true,
  "options": {
    "remote_host": "192.168.20.20",
    "remote_port": 2004,
    "device_link": "/dev/ttyWMA0"
  },
  "schema": {
    "remote_host": "str?",
    "remote_port": "int?",
    "device_link": "str?"
  }
}
